<h3>配送先編集</h3>

<%= form_with model: @address, url: address_path, method: :patch do |f| %>

  <h4>氏名</h4>
  <%= f.text_field :family_name %><%= f.text_field :first_name %>

  <h4>フリガナ</h4>
  <%= f.text_field :family_name_kana %><%= f.text_field :first_name_kana %>

  <h4>郵便番号</h4>
  <%= f.text_field :postal_code %>

  <h4>住所</h4>
  <%= f.text_field :addresse %>

  <h4>電話番号</h4>
  <%= f.text_field :telephone_number %>

  <h4>メールアドレス</h4>
  <%= f.text_field :email %>

  <%= f.submit '編集内容を保存' %>
  

<% end %>
<p>Find me in app/views/public/addresses/edit.html.erb</p>
